{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "^5.2",
		"intervention/image": "~2.1",
		"hieu-le/active": "~2.0",
		"pusher/pusher-php-server": "~2.0",
		"jenssegers/date": "^3.0",
		"league/fractal": "^0.13.0",
		"guzzlehttp/guzzle": "^6.1",
		"intervention/imagecache": "^2.3",
		"bugsnag/bugsnag-laravel": "1.*",
		"iron-io/iron_mq": "~4.0",
		"aws/aws-sdk-php": "^3.15",
		"league/flysystem-aws-s3-v3": "~1.0",
		"doctrine/dbal": "^2.5",
		"stevebauman/location": "^1.3",
		"algolia/algoliasearch-client-php": "^1.8",
		"algolia/algoliasearch-laravel": "^1.0",
		"dingo/api": "1.0.x@dev",
		"tymon/jwt-auth": "^0.5.9",
		"laravelcollective/html": "^5.2",
		"nanigans/single-table-inheritance": "^0.6.1",
		"laravelcollective/iron-queue": "^5.2",
		"themsaid/laravel-langman": "^1.2"
	},
	"require-dev": {
	  	"fzaninotto/faker": "~1.4",
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
        "mockery/mockery": "dev-master",
		"hamcrest/hamcrest-php": "^1.2",
		"laravel/homestead": "^3.0",
		"symfony/dom-crawler": "^3.0",
		"symfony/css-selector": "^3.0"
	},
	"autoload": {
		"classmap": [
			"database"
		],
	  	"files": [
		  "app/helpers.php"
		],
		"psr-4": {
			"EQM\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php",
			"tests/ApiTestCase.php"
		]
	},
	"scripts": {
		"post-root-package-install": [
			"php -r \"copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		],
		"post-install-cmd": [
			"Illuminate\\Foundation\\ComposerScripts::postInstall",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"Illuminate\\Foundation\\ComposerScripts::postUpdate",
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
